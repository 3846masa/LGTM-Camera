{
  "name": "lgtm-camera-on-web",
  "version": "0.0.1",
  "author": "3846masa",
  "browserify": {
    "transform": [
      "./vueify-wrapper.js",
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "gifshot": "global:gifshot"
  },
  "dependencies": {
    "bulma": "0.1.2",
    "eastasianwidth": "0.1.1",
    "file-saver": "1.3.2",
    "font-awesome": "4.6.3",
    "lodash.debounce": "4.0.8",
    "mobile-detect": "1.3.3",
    "normalize.css": "4.2.0",
    "recordrtc": "5.4.0",
    "vue": "2.0.0-rc.6",
    "webrtc-adapter": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-runtime": "6.11.6",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "browserify-shim": "3.8.12",
    "eslint": "3.5.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-html": "1.5.2",
    "eslint-plugin-import": "1.15.0",
    "http-server": "0.9.0",
    "licensify": "3.1.2",
    "minifyify": "7.3.3",
    "ngrok": "2.2.2",
    "npm-run-all": "3.1.0",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.1.2",
    "postcss-reporter": "1.4.1",
    "postcss-url": "5.1.2",
    "pug": "2.0.0-beta6",
    "vueify": "9.2.4",
    "watchify": "3.7.0"
  },
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:browserify",
    "build:browserify": "browserify -vd -p licensify -p [ minifyify --no-map --uglify [ --mangle --compress ] ] -e ./src/index.js > ./lib/index.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:*",
    "watch:browserify": "watchify -vd -p licensify -p [ minifyify --map index.min.js.map --output ./lib/index.min.js.map --uglify [ --mangle --compress ] ] -e ./src/index.js -o ./lib/index.min.js",
    "watch:serve": "npm-run-all -p watch:serve:*",
    "watch:serve:server": "http-server -c -1 -p 8000 -a 0.0.0.0",
    "watch:serve:ngrok": "ngrok http 8000"
  }
}
